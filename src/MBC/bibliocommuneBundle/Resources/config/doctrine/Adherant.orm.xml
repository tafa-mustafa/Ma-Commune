<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="MBC\bibliocommuneBundle\Entity\Adherant" table="adherant">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="prenom" type="string" column="prenom" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nom" type="string" column="nom" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tel" type="integer" column="Tel" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="address" type="string" column="address" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dateInscription" type="date" column="date_Inscription" nullable="false"/>
    <field name="nombreEmprunEnCour" type="integer" column="nombre_emprun_en_cour" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
